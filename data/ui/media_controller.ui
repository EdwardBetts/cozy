<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="0.0"/>
  <object class="GtkImage" id="next_img">
    <property name="icon_name">object-rotate-right-symbolic</property>
  </object>
  <object class="GtkImage" id="next_img1">
    <property name="icon_name">object-rotate-right-symbolic</property>
  </object>
  <object class="GtkImage" id="play_img">
    <property name="icon_name">play-symbolic</property>
  </object>
  <object class="GtkImage" id="play_img1">
    <property name="icon_name">play-symbolic</property>
  </object>
  <object class="GtkImage" id="prev_img">
    <property name="icon_name">object-rotate-left-symbolic</property>
  </object>
  <template class="MediaController" parent="AdwSqueezer">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="hexpand">True</property>
    <property name="homogeneous">True</property>
    <property name="interpolate_size">True</property>
    <child>
      <object class="GtkBox" id="big_player">
        <child>
          <object class="GtkBox" id="mini_player">
            <property name="halign">start</property>
            <property name="spacing">6</property>
            <child>
              <object class="GtkBox" id="control_box_big">
                <property name="halign">end</property>
                <property name="valign">center</property>
                <property name="spacing">3</property>
                <child>
                  <object class="GtkBox">
                    <property name="hexpand">1</property>
                    <child>
                      <object class="GtkButton" id="prev_button_big">
                        <property name="sensitive">0</property>
                        <property name="focusable">1</property>
                        <property name="receives_default">1</property>
                        <property name="tooltip_text" translatable="1">Rewind</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="hexpand">1</property>
                        <property name="image">prev_img</property>
                        <child internal-child="accessible">
                          <object class="AtkObject" id="prev_button_big-atkobject">
                            <property name="AtkObject::accessible-name" translatable="1">Rewind button</property>
                            <property name="AtkObject::accessible-description" translatable="1">Rewind playback</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="play_button_big">
                        <property name="width_request">42</property>
                        <property name="sensitive">0</property>
                        <property name="focusable">1</property>
                        <property name="receives_default">1</property>
                        <property name="tooltip_text" translatable="1">Start playback</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="hexpand">1</property>
                        <property name="image">play_img</property>
                        <child internal-child="accessible">
                          <object class="AtkObject" id="play_button_big-atkobject">
                            <property name="AtkObject::accessible-name" translatable="1">Play/Pause Button</property>
                            <property name="AtkObject::accessible-description" translatable="1">Start or pause the playback</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="next_button_big">
                        <property name="sensitive">0</property>
                        <property name="focusable">1</property>
                        <property name="receives_default">1</property>
                        <property name="tooltip_text" translatable="1">Forward</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="hexpand">1</property>
                        <property name="image">next_img</property>
                        <child internal-child="accessible">
                          <object class="AtkObject" id="next_button_big-atkobject">
                            <property name="AtkObject::accessible-name" translatable="1">Forward button</property>
                            <property name="AtkObject::accessible-description" translatable="1">Forward Playback</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <style>
                      <class name="linked"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="cover_img_event_box_big">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                  <object class="GtkImage" id="cover_img_big">
                    <property name="width_request">45</property>
                    <property name="height_request">45</property>
                    <property name="halign">center</property>
                    <property name="valign">end</property>
                    <property name="stock">gtk-missing-image</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="title_box">
                <property name="name">s</property>
                <property name="tooltip_text" translatable="1">Currently playing</property>
                <property name="halign">center</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel" id="title_label">
                    <property name="vexpand">1</property>
                    <property name="name">title_book</property>
                    <property name="label" translatable="0">Harry Potter und der Stein der Weisen</property>
                    <property name="ellipsize">end</property>
                    <property name="width_chars">20</property>
                    <property name="single_line_mode">1</property>
                    <property name="xalign">0</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="title_label-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Booktitle</property>
                        <property name="AtkObject::accessible-description" translatable="1">Title of currently playing book</property>
                      </object>
                    </child>
                    <style>
                      <class name="title"/>
                      <class name="no_padding"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="subtitle_label">
                    <property name="vexpand">1</property>
                    <property name="name">title_track</property>
                    <property name="label" translatable="0">Kapitel 01</property>
                    <property name="ellipsize">end</property>
                    <property name="width_chars">20</property>
                    <property name="single_line_mode">1</property>
                    <property name="xalign">0</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="subtitle_label-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Part name</property>
                        <property name="AtkObject::accessible-description" translatable="1">Title of the currently playing part</property>
                      </object>
                    </child>
                    <style>
                      <class name="subtitle"/>
                      <class name="no_padding"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="SeekBar">
            <property name="hexpand">1</property>
            <property name="spacing">5</property>
            <child>
              <object class="GtkLabel" id="current_label">
                <property name="tooltip_text" translatable="1">Elapsed time</property>
                <property name="halign">end</property>
                <property name="valign">center</property>
                <property name="label">&lt;span font_features=&apos;tnum&apos;&gt;--:--&lt;/span&gt;</property>
                <property name="use_markup">1</property>
                <property name="single_line_mode">1</property>
                <child internal-child="accessible">
                  <object class="AtkObject" id="current_label-atkobject">
                    <property name="AtkObject::accessible-name" translatable="1">Time elapsed</property>
                    <property name="AtkObject::accessible-description" translatable="1">Elapsed time of current part</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="progress_scale">
                <property name="width_request">150</property>
                <property name="sensitive">0</property>
                <property name="focusable">1</property>
                <property name="tooltip_text" translatable="1">Jump to position in current chapter</property>
                <property name="hexpand">1</property>
                <property name="lower_stepper_sensitivity">on</property>
                <property name="upper_stepper_sensitivity">on</property>
                <property name="restrict_to_fill_level">0</property>
                <property name="fill_level">0</property>
                <child internal-child="accessible">
                  <object class="AtkObject" id="progress_scale-atkobject">
                    <property name="AtkObject::accessible-name" translatable="1">Position slider</property>
                    <property name="AtkObject::accessible-description" translatable="1">Position of the current part in seconds</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="remaining_event_box">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkLabel" id="remaining_label">
                    <property name="tooltip_text" translatable="1">Remaining time</property>
                    <property name="halign">start</property>
                    <property name="label">&lt;span font_features=&apos;tnum&apos;&gt;--:--&lt;/span&gt;</property>
                    <property name="use_markup">1</property>
                    <property name="single_line_mode">1</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="remaining_label-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Time remaining</property>
                        <property name="AtkObject::accessible-description" translatable="1">Remaining time of current part</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="tools_box_big">
            <property name="valign">center</property>
            <property name="spacing">3</property>
            <child>
              <object class="GtkVolumeButton" id="volume_button">
                <property name="sensitive">0</property>
                <property name="focusable">1</property>
                <property name="focus_on_click">0</property>
                <property name="receives_default">1</property>
                <property name="tooltip_text" translatable="1">Volume control</property>
                <property name="size">button</property>
                <property name="icons">audio-volume-muted-symbolic
audio-volume-high-symbolic
audio-volume-low-symbolic
audio-volume-medium-symbolic</property>
                <child internal-child="plus_button">
                  <object class="GtkButton">
                    <property name="visible">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="halign">center</property>
                    <property name="valign">center</property>
                    <property name="relief">none</property>
                  </object>
                </child>
                <child internal-child="minus_button">
                  <object class="GtkButton">
                    <property name="visible">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="halign">center</property>
                    <property name="valign">center</property>
                    <property name="relief">none</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuButton" id="playback_speed_button_big">
                <property name="sensitive">0</property>
                <property name="focusable">1</property>
                <property name="receives_default">1</property>
                <property name="tooltip_text" translatable="1">Playback speed</property>
                <child>
                  <object class="GtkImage" id="playback_speed_img">
                    <property name="icon_name">playback-speed-symbolic</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuButton" id="timer_button">
                <property name="sensitive">0</property>
                <property name="focusable">1</property>
                <property name="receives_default">1</property>
                <property name="tooltip_text" translatable="1">Sleep timer</property>
                <child>
                  <object class="GtkImage" id="timer_image">
                    <property name="icon_name">bed-symbolic</property>
                  </object>
                </child>
                <child internal-child="accessible">
                  <object class="AtkObject" id="timer_button-atkobject">
                    <property name="AtkObject::accessible-name" translatable="1">Timer menu button</property>
                    <property name="AtkObject::accessible-description" translatable="1">Open the sleep timer popover</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox" id="small_player">
        <property name="hexpand">1</property>
        <child>
          <object class="GtkBox" id="cover_img_event_box_small">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <child>
              <object class="GtkImage" id="cover_img_small">
                <property name="width_request">45</property>
                <property name="height_request">45</property>
                <property name="halign">start</property>
                <property name="valign">end</property>
                <property name="stock">gtk-missing-image</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="control_box_small">
            <property name="hexpand">1</property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <property name="spacing">3</property>
            <child>
              <object class="GtkBox">
                <property name="hexpand">1</property>
                <child>
                  <object class="GtkButton" id="prev_button_small">
                    <property name="sensitive">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="tooltip_text" translatable="1">Rewind</property>
                    <property name="halign">center</property>
                    <property name="valign">center</property>
                    <property name="hexpand">1</property>
                    <property name="image">prev_img</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="prev_button_small-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Rewind button</property>
                        <property name="AtkObject::accessible-description" translatable="1">Rewind playback</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="play_button_small">
                    <property name="width_request">42</property>
                    <property name="sensitive">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="tooltip_text" translatable="1">Start playback</property>
                    <property name="halign">center</property>
                    <property name="valign">center</property>
                    <property name="hexpand">1</property>
                    <property name="image">play_img</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="play_button_small-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Play/Pause Button</property>
                        <property name="AtkObject::accessible-description" translatable="1">Start or pause the playback</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="next_button_small">
                    <property name="sensitive">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="tooltip_text" translatable="1">Forward</property>
                    <property name="halign">center</property>
                    <property name="valign">center</property>
                    <property name="hexpand">1</property>
                    <property name="image">next_img</property>
                    <child internal-child="accessible">
                      <object class="AtkObject" id="next_button_small-atkobject">
                        <property name="AtkObject::accessible-name" translatable="1">Forward button</property>
                        <property name="AtkObject::accessible-description" translatable="1">Forward Playback</property>
                      </object>
                    </child>
                  </object>
                </child>
                <style>
                  <class name="linked"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkMenuButton" id="playback_speed_button_small">
            <property name="sensitive">0</property>
            <property name="focusable">1</property>
            <property name="receives_default">1</property>
            <property name="tooltip_text" translatable="1">Playback speed</property>
            <property name="halign">end</property>
            <child>
              <object class="GtkImage" id="playback_speed_img1">
                <property name="icon_name">playback-speed-symbolic</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
  </template>
  <object class="GtkImage" id="prev_img1">
    <property name="icon_name">object-rotate-left-symbolic</property>
  </object>
</interface>
